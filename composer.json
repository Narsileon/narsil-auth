{
	"name": "narsil/auth",
	"description": "Narsil Auth",
	"license": "MIT",
	"keywords": [
		"auth",
		"inertia",
		"laravel",
		"narsil",
		"php",
		"react"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "../narsil-forms"
		},
		{
			"type": "path",
			"url": "../narsil-localization"
		},
		{
			"type": "path",
			"url": "../narsil-menus"
		},
		{
			"type": "path",
			"url": "../narsil-policies"
		},
		{
			"type": "path",
			"url": "../narsil-settings"
		},
		{
			"type": "path",
			"url": "../narsil-storage"
		},
		{
			"type": "path",
			"url": "../narsil-tables"
		},
		{
			"type": "path",
			"url": "../narsil-tree"
		},
		{
			"type": "path",
			"url": "../narsil-ui"
		}
	],
	"require": {
		"php": "^8.2",
		"inertiajs/inertia-laravel": "^1.0",
		"laravel/fortify": "^1.21",
		"laravel/framework": "^11.3",
		"mobiledetect/mobiledetectlib": "^4.8",
		"narsil/forms": "dev-main",
		"narsil/localization": "dev-main",
		"narsil/menus": "dev-main",
		"narsil/policies": "dev-main",
		"narsil/settings": "dev-main",
		"narsil/ui": "dev-main"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\Auth\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\Auth\\NarsilAuthServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	}
}
